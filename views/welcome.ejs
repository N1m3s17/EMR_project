<!doctype html>
<html lang="en">
  <head>
    <%- include('partials/head'); %>
  </head>
  <body>
    <div class="container login-container">
        <div class="row">
            <div class="col-md-6 login-form-1">
            </div>
            <div class="col-md-6 login-form-2">
                <h3>Login</h3>
                <form class="container" method="post" action="auth">
                    <div class="form-group">
                        <input type="text" name="username" class="form-control" placeholder="Your Email *" value="" required/>
                    </div>
                    <div class="form-group">
                        <input type="password" name="username" class="form-control" placeholder="Your Password *" value="" required/>
                    </div>
                    <div class="form-group">
                        <input type="submit" class="btnSubmit" value="Login" />
                    </div>
                    <div class="form-group">
                        <a href="#" class="ForgetPwd" value="Login">Forgot Password?</a>
                    </div>
                </form>
            </div>
        </div>
        </div>
    </div>
    <%- include('partials/footer'); %>
  </body>
</html>